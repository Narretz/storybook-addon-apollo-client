{
  "author": {
    "name": "Aaron Reisman",
    "email": "aaron@lifeiscontent.net",
    "url": "https://lifeiscontent.net/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lifeiscontent/storybook-addon-apollo-client.git"
  },
  "devDependencies": {
    "@apollo/client": "3.x",
    "@types/jest": "25.1.4",
    "@types/react-syntax-highlighter": "^13.5.0",
    "@types/webpack-env": "^1.15.3",
    "graphql": "15.3.0",
    "husky": "4.2.3",
    "npm-run-all": "^4.1.5",
    "react": "16.x",
    "react-dom": "16.13.1",
    "rimraf": "^3.0.2",
    "tsdx": "0.13.0",
    "tslib": "1.11.1",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "@storybook/addons": "6.x",
    "@storybook/components": "6.x"
  },
  "engines": {
    "node": ">=10"
  },
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "name": "storybook-addon-apollo-client",
  "peerDependencies": {
    "@apollo/client": "3.x",
    "react": "16.x"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "scripts": {
    "start": "npm run build:esm -- --watch",
    "build": "rimraf dist && run-p build:*",
    "build:esm": "tsc -p tsconfig.build.json --outDir dist/esm",
    "build:cjs": "tsc -p tsconfig.build.json --module commonjs --target es5 --outDir dist/cjs",
    "test": "tsdx test",
    "lint": "tsdx lint src test",
    "prepare": "npm run build"
  },
  "version": "3.0.0"
}
